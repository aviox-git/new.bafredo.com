            <div class="row">
                {% for product in products %}
                    <div class="col-12 col-lg-3 col-md-4 col-sm-6">
                        <div class="view-products-single-box">
                            <div class="view-products-thumb">
                                <img src="{{ asset("uploads/product/thumbnail/#{product.getImage()}") }}">
                                <div class="view-products-hover-info">
                                    <a href="#" v-on:click="addToCart({{ product.toJson() }})" class="icon-link-btn" data-toggle="tooltip" data-placement="top" title="Add to cart"> 

                                    <i class="fa fa-shopping-cart pr-2"></i> Add to cart</a>
                                </div>
                            </div>
                            <div class="view-products-info">                            
                                <div class="view-products-title">
                                    <h3><a href=#"> {{ product.getName() }} </a></h3>
                                    <span class="view-products-price">{{ product.getCurrency() ~' '~ product.getPrice() | round }}</span>
                                </div>
                            </div>
                            <div class="hover-icon-link-box">                            
                                

                                <a href="#" v-on:click="addToWishList({{ product.id }})" class="hover-icon-link" data-toggle="tooltip" data-placement="top" title="Login to use whishlist">   

